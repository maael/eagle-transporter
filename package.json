{
  "name": "@maael/eagle-transporter",
  "version": "1.0.0",
  "description": "A tool to connect data, transform it, and transport it to its destination.",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:js": "browserify app/main.js -t [ babelify --presets [es2015 react] ] -t [ envify --NODE_ENV $ENV ] -o public/js/bundle.js",
    "postinstall": "npm run build",
    "start": "node server.js | bunyan",
    "test": "npm run lint && npm run test:client && npm run test:server",
    "test:client": "mocha test/client --recursive --compilers js:babel-register",
    "test:server": "mocha test/server --recursive --compilers js:babel-register",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "watchify app/main.js -t [ babelify --presets [es2015 react] ] -v -o public/js/bundle.js",
    "lint": "standard"
  },
  "dependencies": {
    "@maael/express-bunyan": "^1.0.0",
    "archiver": "^1.3.0",
    "async": "^1.5.2",
    "aws-sdk": "^2.50.0",
    "babel-core": "^6.7.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.1",
    "browserify": "^13.0.0",
    "bunyan": "^1.8.10",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.1",
    "dotenv": "^2.0.0",
    "envify": "^4.0.0",
    "express": "^4.13.4",
    "express-validator": "^2.20.4",
    "jade": "^1.11.0",
    "jdenticon": "^1.4.0",
    "js-md5": "^0.4.2",
    "jsonwebtoken": "^5.7.0",
    "less-middleware": "^2.2.0",
    "moment": "^2.12.0",
    "mongoose": "^4.10.4",
    "nodemailer": "^2.3.0",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.8.0",
    "react": "^15.4.2",
    "react-cookie": "^0.4.5",
    "react-dom": "^15.4.2",
    "react-redux": "4.4.1",
    "react-router": "^2.4.0",
    "react-svg-inline": "^2.0.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "request": "^2.69.0",
    "semantic-ui-less": "^2.2.10",
    "stream-to-array": "^2.3.0",
    "watchify": "^3.7.0",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-plugin-rewire": "1.0.0-rc-3",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "fetch-mock": "3.0.2",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.2",
    "redux-mock-store": "^1.0.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "standard": "^10.0.2",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "private": true,
  "standard": {
    "globals": [
      "describe",
      "it",
      "afterEach",
      "fetch"
    ]
  }
}
